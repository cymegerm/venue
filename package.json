{
  "name": "venue",
  "version": "0.0.1",
  "description": "A simple concert ticket reservation app which serves as a playground for testing new tech, mainly Next.js",
  "license": "MIT",
  "author": "Simon St-Germain",
  "scripts": {
    "start": "nx serve",
    "start:all": "concurrently -r -g -k \"npm run storybook\" \"npm run start\"",
    "build": "nx build",
    "build:docker": "next build",
    "test": "nx test",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "tailwind:dev": "npx tailwindcss -i ./apps/venue/tailwind/input.css -o ./apps/venue/tailwind/output.css --watch",
    "tailwind": "npx tailwindcss -i ./tailwind/input.css -o ./tailwind/output.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook --browsers chromium firefox webkit",
    "test-storybook:watch": "test-storybook --browsers chromium firefox webkit --watch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "private": true,
  "dependencies": {
    "@nrwl/next": "14.8.3",
    "@tanstack/react-query": "^4.10.3",
    "@tanstack/react-query-devtools": "^4.10.1",
    "core-js": "^3.25.5",
    "daisyui": "^2.31.0",
    "gaxios": "^5.0.2",
    "next": "12.3.1",
    "next-auth": "^4.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "sharp": "^0.31.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@nrwl/cli": "14.8.3",
    "@nrwl/cypress": "14.8.3",
    "@nrwl/eslint-plugin-nx": "14.8.3",
    "@nrwl/jest": "14.8.3",
    "@nrwl/linter": "14.8.3",
    "@nrwl/nx-cloud": "14.7.0",
    "@nrwl/react": "14.8.3",
    "@nrwl/web": "14.8.3",
    "@nrwl/workspace": "14.8.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/jest": "0.0.10",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/test-runner": "^0.7.2",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "babel-jest": "28.1.3",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.5",
    "husky": "^8.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "nx": "14.8.3",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.1.8",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
